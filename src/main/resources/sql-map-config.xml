<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.shoe.biz.product.ProductVO" alias="product"></typeAlias>
		<typeAlias type="com.shoe.biz.member.MemberVO" alias="member"></typeAlias>
		<typeAlias type="com.shoe.biz.address.AddressVO" alias="address"></typeAlias>
		<typeAlias type="com.shoe.biz.enter.EnterVO" alias="enter"></typeAlias>
		<typeAlias type="com.shoe.biz.store.StoreVO" alias="store"></typeAlias>
		<typeAlias type="com.shoe.biz.buy.BuyVO" alias="buy"></typeAlias>
		
	</typeAliases>

	<mappers>
		<mapper resource="mappings/member-mapping.xml" />
		<mapper resource="mappings/product-mapping.xml" />
		<mapper resource="mappings/enter-mapping.xml" />
		<mapper resource="mappings/store-mapping.xml" />
		<mapper resource="mappings/buy-mapping.xml" />
	</mappers>
</configuration>
